Combat.TextScaled = true
Combat.TextSize = 14.000
Combat.TextWrapped = true

Open1.Name = "Open1"
Open1.Parent = Main
Open1.BackgroundColor3 = Color3.fromRGB(213, 0, 0)
Open1.Position = UDim2.new(0.785714209, 0, 0, 0)
Open1.Size = UDim2.new(0, 24, 0, 26)
Open1.Font = Enum.Font.SourceSans
Open1.Text = "+"
Open1.TextColor3 = Color3.fromRGB(0, 0, 0)
Open1.TextScaled = true
Open1.TextSize = 14.000
Open1.TextWrapped = true
Open1.MouseButton1Click:Connect(function()
	Open1.Visible = false
	Close1.Visible = true
	if AimAssist.Visible == false then
	AimAssist.Visible = true
	AutoClicker.Visible = true
	KillAura.Visible = true
	AntiKnockback.Visible = true
		NoClickDelay.Visible = true	
		end
end)




